Below are the SQL Database Queries for creating the "ticketbookingapi" Schema & table "ticketbookingserviceuserregistration"
---------------------------------------------------------------------------------------------------------------------------------
use ticketbookingapi;
create table TicketBookingServiceUserRegistration(user_id varchar(255) not null, user_name varchar(255) not null,
email varchar(255) not null, first_name varchar(255) not null,
last_name varchar(255) not null, mobile varchar(10) not null,
password varchar(255) not null, primary key (user_id), UNIQUE(user_name));
select * from TicketBookingServiceUserRegistration;
--------------------------------------------------------------------------------------------------------------------------------
use ticketbookingapi;
select * from ticketbookingserviceuserregistration;
--------------------------------------------------------------------------------------------------------------------------------
ALTER TABLE ticketbookingserviceuserregistration
MODIFY COLUMN user_id BIGINT NOT NULL AUTO_INCREMENT;
--------------------------------------------------------------------------------------------------------------------------------
delete from ticketbookingserviceuserregistration where user_id = 1;
delete from ticketbookingserviceuserregistration where user_name = 'U1Jhon@4';
--------------------------------------------------------------------------------------------------------------------------------

DROP TABLE TicketBookingServiceUserRegistration;

ALTER TABLE ticketbookingserviceuserregistration DROP COLUMN userid;
